    "                f.write(f'      <edoal:measure>{mapping.get(\"combined_score\", 0.5):.3f}</edoal:measure>\\n')\n",
    "                f.write(f'    </edoal:Cell>\\n')\n",
    "                f.write(f'  </edoal:map>\\n')\n",
